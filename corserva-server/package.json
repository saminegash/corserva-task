{
  "name": "corserva-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "nodemon src/index.ts",
    "db:setup": "sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:migration:down": "sequelize db:migrate:undo:all",
    "db:refresh": "npm run db:migrate:down && npm run db:migrate",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
